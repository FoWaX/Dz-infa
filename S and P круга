
#define _USE_MATH_DEFINES
#include <iostream>
using namespace std;
#include <iomanip>
int main()
{
	setlocale(LC_ALL, "Rus");
	int r;
	cin >> r;
	cout << "Площадь круга равна ";
	cout << fixed << setprecision(4) << M_PI * r * r << endl;
	cout << "Длина окружности равна ";
	cout << M_PI * 2 * r;
}

